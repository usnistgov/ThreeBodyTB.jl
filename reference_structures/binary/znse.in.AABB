&control
    calculation = 'scf'
    restart_mode='from_scratch',
    prefix = 'qe2'
    tstress = .true.
    tprnfor = .true.
    pseudo_dir = '/users/kfg/psuedo/gbrv_1.5/',
!    pseudo_dir = '/home/kevin/psp/'
    outdir = '/tmp'
    disk_io = 'low'
    wf_collect = .true.
    verbosity = 'high'    
 /
&system
    ibrav=  0, 
    nat=  2 
    ntyp= 2
    ecutwfc= 45,
    ecutrho= 250,
    q2sigma= 1
    ecfixed= 44.5,
    qcutz= 800
    occupations= "smearing",
    degauss= 0.005
    lda_plus_u= .false.
    force_symmorphic = true
/
&electrons
    diagonalization='david'
    mixing_mode = 'local-TF'
    mixing_beta = 0.3
    conv_thr = 1d-9
 /
&ions
